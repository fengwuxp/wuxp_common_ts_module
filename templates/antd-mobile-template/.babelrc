{
  "presets": [
    "react",
    "env"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],

    [
      "import",
      {
        "libraryName": "antd-mobile",
        "libraryDirectory": "lib",
        "style": true
      }
    ]
  ]
}
